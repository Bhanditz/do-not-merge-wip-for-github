{
  "name": "do-not-merge-wip-for-github",
  "description": "Do not merge *WIP* pull request!",
  "version": "2.0.0-alpha.1",
  "author": {
    "name": "sanemat",
    "email": "ogataken@gmail.com",
    "url": "sane.jp"
  },
  "dependencies": {},
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.5",
    "eclint": "^2.5.0",
    "eslint": "^4.14.0",
    "fixpack": "^2.3.1",
    "globstar": "^1.0.0",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "repository": "sanemat/do-not-merge-wip-for-github",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "changelog": "conventional-changelog -i CHANGELOG.md --same-file --preset angular",
    "eclint": "globstar --ignore \"node_modules/**\" -- eclint check \"*.{js,yml,md,json}\"",
    "eslint": "eslint . --ignore-path .gitignore",
    "fixpack": "fixpack",
    "lint": "npm run eclint && npm run eslint",
    "verify": "npm run lint"
  }
}
